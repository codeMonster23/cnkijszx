}

.menu .item .submenu {
  position: absolute;
  right: -725px;
  top: 50%;
  z-index: 10;
  width: 663px;
  padding: 0 30px;
  border: 1px solid #22b17c;
  background: #fff;
  display: none;
}

.menu .item .submenu .list-item {
