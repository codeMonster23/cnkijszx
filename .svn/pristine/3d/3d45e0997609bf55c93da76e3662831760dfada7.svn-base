}

.menu .item .submenu .list-item .title a {
  color: #3c3c3c;
}

.menu .item .submenu .list-item ul {
  overflow: hidden;
}

.menu .item .submenu .list-item ul li {
  float: left;
}

.menu .item .submenu .list-item ul a {
  padding: 0 9px;
  border-left: 1px solid #c8c8c8;
  color: #787878;
}

.carousel {
  position: relative;
  width: 100%;
  height: 300px;
}

.carousel ul li {
  position: absolute;
  top: 0;
  left: 0;
  display: none;
}

.carousel ul li.first {
  display: block;
}

.carousel ol.circleList {
  position: absolute;
  bottom: 10px;
  left: 50%;
  margin-left: -27px;
  width: 54px;
  height: 10px;
}

.carousel ol.circleList li {
  float: left;
  width: 10px;
  height: 10px;
  background-color: #787878;
  margin: 0 4px;
  border-radius: 50%;
  cursor: pointer;
}

.carousel ol.circleList li.cur {
  background-color: #fff;
}

.carousel2 {
  height: 260px;
}

.carousel2 ol.circleList {
  margin-left: -36px;
  width: 72px;
}

.carousel2 ol.circleList li {
  background-color: #bfbfbf;
}

.carousel2 ol.circleList li.cur {
  background-color: #22b17c;
}

